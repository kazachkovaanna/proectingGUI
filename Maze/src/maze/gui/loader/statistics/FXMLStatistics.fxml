<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="maze.gui.controllers.FXMLStatisticsController">
   <top>
      <Button mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT" onAction="#handleBackButtonAction" text="Назад" BorderPane.alignment="CENTER_LEFT">
         <BorderPane.margin>
            <Insets bottom="5.0" left="5.0" top="5.0" />
         </BorderPane.margin></Button>
   </top>
   <center>
      <TableView fx:id="table" prefHeight="203.0" prefWidth="149.0" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="level" prefWidth="75.0" text="Уровень" />
            <TableColumn fx:id="time" prefWidth="75.0" text="Время" />
          <TableColumn fx:id="steps" prefWidth="75.0" text="Шаги" />
            <TableColumn fx:id="hints" prefWidth="75.0" text="Подсказки" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </center>
</BorderPane>
